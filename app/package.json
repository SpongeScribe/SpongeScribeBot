{
  "name": "spongescribe",
  "type": "module",
  "version": "0.1.0",
  "description": "Strangetext Transcription",
  "main": "index.js",
  "files": [
    "package.json"
  ],
  "module": "main.js",
  "scripts": {
    "dev": "node -r dotenv/config dist/index.js ;",
    "sleep": "node sleep.js ; ",
    "build": "rm -rf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files ; ",
    "start": "npm run-script build ; npm run-script start-dist ; ",
    "start-dist": "node dist/index.js ; ",
    "start-slow": "npm run-script start-dist ; npm run-script sleep ; ",
    "test": "echo \"Error: no test specified\" && exit 1 ; ",
    "twitter": "npm run-script build ; node -r dotenv/config dist/twitter.js ; ",
    "version": "echo $npm_package_version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SpongeScribe/SpongeScribeBot.git"
  },
  "keywords": [
    "yes",
    "accessibility",
    "text2png",
    "transcription",
    "image generation",
    "twitter",
    "bot"
  ],
  "author": "dezren39",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/SpongeScribe/SpongeScribeBot/issues"
  },
  "homepage": "https://github.com/SpongeScribe/SpongeScribeBot#readme",
  "dependencies": {
    "esm": "^3.2.25",
    "hashish": "0.0.4",
    "mongodb": "^3.5.9",
    "move-file": "^2.0.0",
    "seq": "^0.3.5",
    "text2png": "^2.3.0",
    "twitter-lite": "^0.14.0",
    "unescape-js": "^1.1.4",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/node": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "dotenv": "^8.2.0"
  }
}
