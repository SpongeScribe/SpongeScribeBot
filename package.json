{
  "name": "spongescribe",
  "type": "module",
  "version": "0.2.0",
  "description": "Strangetext Transcription",
  "main": "index.js",
  "exports": {
    ".": "./index.js",
    "./index": "./index.js",
    "./index.js": "./index.js",
    "./sleep": "./sleep.js",
    "./sleep.js": "./sleep.js",
    "./twitter": "./twitter.js",
    "./twitter.js": "./twitter.js",
    "./twitter-autohook": "./twitter-autohook.js",
    "./twitter-autohook.js": "./twitter-autohook.js",
    "./modules/": "./modules/",
    "./modules/sleep": "./modules/sleep.js",
    "./modules/sleep.js": "./modules/sleep.js",
    "./modules/image-generation": "./modules/image-generation.js",
    "./modules/image-generation.js": "./modules/image-generation.js",
    "./package.json": "./package.json",
    "./lib": "./lib/index.js",
    "./lib/index": "./lib/index.js",
    "./lib/index.js": "./lib/index.js",
    "./lib/sleep": "./lib/sleep.js",
    "./lib/sleep.js": "./lib/sleep.js",
    "./lib/twitter": "./lib/twitter.js",
    "./lib/twitter.js": "./lib/twitter.js",
    "./lib/twitter-autohook": "./lib/twitter-autohook.js",
    "./lib/twitter-autohook.js": "./lib/twitter-autohook.js",
    "./lib/modules/": "./lib/modules/",
    "./lib/modules/sleep": "./lib/modules/sleep.js",
    "./lib/modules/sleep.js": "./lib/modules/sleep.js",
    "./lib/modules/image-generation": "./lib/modules/image-generation.js",
    "./lib/modules/image-generation.js": "./lib/modules/image-generation.js",
    "./lib/package.json": "./lib/package.json"
  },
  "files": [
    "package.json",
    "package-lock.json",
    "index.js",
    "sleep.js",
    "twitter.js",
    "twitter.autohook.js",
    "modules/image-generation.js",
    "modules/sleep.js"
  ],
  "module": "main.js",
  "scripts": {
    "dev": "node --unhandled-rejections=strict --trace-warnings -r dotenv/config index.js ;",
    "sleep": "node --unhandled-rejections=strict --trace-warnings sleep.js ; ",
    "build": "babel ./ --out-dir lib/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log,./lib/lib,./lib/node_modules --copy-files ; rm -f lib/package.json; echo \"{ \\\"type\\\": \\\"commonjs\\\" }\" > lib/package.json",
    "start": "node index.js ; ",
    "build-start": "npm run-script build ; npm run-script start-lib ; ",
    "start-lib": "node --unhandled-rejections=strict --trace-warnings lib/index.js ; ",
    "start-slow": "npm run-script start-lib ; npm run-script sleep ; ",
    "test": "echo \"Error: no test specified\" && exit 1 ; ",
    "twitter": "npm run-script build ; node --unhandled-rejections=strict --trace-warnings -r dotenv/config twitter.js ; ",
    "version": "echo $npm_package_version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SpongeScribe/SpongeScribeBot.git"
  },
  "keywords": [
    "yes",
    "accessibility",
    "text2png",
    "transcription",
    "image generation",
    "twitter",
    "bot"
  ],
  "author": "dezren39",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/SpongeScribe/SpongeScribeBot/issues"
  },
  "homepage": "https://github.com/SpongeScribe/SpongeScribeBot#readme",
  "dependencies": {
    "esm": "^3.2.25",
    "hashish": "0.0.4",
    "mongodb": "^3.5.9",
    "move-file": "^2.0.0",
    "seq": "^0.3.5",
    "text2png": "^2.3.0",
    "twitter-autohook": "^1.7.1",
    "twitter-lite": "^0.14.0",
    "unescape-js": "^1.1.4",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/node": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "dotenv": "^8.2.0"
  }
}
